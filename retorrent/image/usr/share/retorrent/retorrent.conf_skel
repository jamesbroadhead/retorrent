[retorrent]
torrentfilesdir='~/torrents/torrentfiles'
seeddir='~/seed'
seedtorrentfilesdir='~/seed/torrentfiles'

[movies]
paths='/mnt/foo/video/movies'
treat_as='movies'

[tv]
paths='/mnt/foo/video/tv','/mnt/bar/video/tv'
treat_as='tv'

[documentaries]
paths='/mnt/foo/video/documentaries','/mnt/bar/video/documentaries'
treat_as='tv'
